<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Pennsouth\MdsBundle\Entity\AptSize" table="apt_size">
    <id name="bedroomCount" type="integer" column="Bedroom_Count">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="aptSizeDescription" type="string" column="Apt_Size_Description" length="20" nullable="true">
      <options>
        <option name="fixed"/>
      </options>
    </field>
  </entity>
</doctrine-mapping>
